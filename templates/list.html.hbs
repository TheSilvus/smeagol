{{#> base.html}}

    {{#*inline "title"}}
        Listing {{path}} - Smeagol
    {{/inline}}

    {{#*inline "page"}}
        <h1>Listing {{path}}</h1>

        <ul>
            {{#each children}}
                <li><a href="{{this.link}}">{{this.name}}</a></li>    
            {{/each}}
        </ul>
    {{/inline}}

{{/base.html}}
